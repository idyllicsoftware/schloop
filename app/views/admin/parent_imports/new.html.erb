<div id="studentUploadWrapperId" class="school-main-section">
  <div class="container">
    <div class="text-center mb50">
      <h3>Upload <%= @grade.name %> CSV</h3>
      <p>Uploading  a  file  will  update  an  existing  student  profile  if  it  exists.  <br/>For  new records, they will be added to the current list</p>
    </div>
    <%= form_tag '', class:"fileDetails uploadFiles_btn_container clearfix", novalidate: true do %>
        <%= hidden_field_tag :school_id,  @school.id%>
        <%= hidden_field_tag :grade_id,  @grade.id%>
        <div class="file_upload_files--upload-block">
          <div>
            <div class="fileUpload file_upload_files--fileUploadBtn">
              <span><i class="fa fa-cloud-upload"></i> Choose File</span>
              <input class="fileuploadInput upload" type="file" name="parent_import[file]" data-url="/admin/parent_imports" accept=".csv">
            </div>
            <button class="btn btn-primary text-uppercase disabled uploadBtn">upload</button>
          </div>
        <div class="progressWrap" style="padding: 15px 0px;display: none;">
          <div class="progress">
            <div class="progress-bar progress-bar-success progress-bar-striped active" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%">
              <span></span>
            </div>
          </div>
        </div>
        <table class="selected_files"></table>
        </div>
    <% end %>
  </div>
</div>