<script type="text/html" id="collaborations_bookmarks_tpl">
	{{#data}}
	<div class="schloopmark-item">
		{{#bookmark_data}}
		<div class="schloopmark-view-section">
			<div class="schloopmark-view">
  				<div class="collaborated-by-teacher-img" style="background-image: url('https://dummyimage.com/45x45/2abdda/0011ff&text=1');">
  				</div>
  				<div class="collaborated-by-teacher-detail">
  					<p>{{title}}</p>
  					<div class="about-schloopmark"><time class="timeago" datetime=".{{#collaboration_data}} {{created_at}} {{/collaboration_data}}" title="id"></time> |<span> {{grade_name}} </span>|<span> {{subject_name}} </span>|<span> {{topic_name}} </span></div>
  				</div>
			</div>
			<div class="schloopmark-content">
				<p>{{caption}}</p>
				<div class="content-block">
				{{#data}}
					{{data}}
				{{/data}}
				{{#preview_image_url}}
					<img src="{{preview_image_url}}" style="width: 300px; height: 200px;">
				{{/preview_image_url}}
				</div>
			</div>	
		</div>
		{{/bookmark_data}}
		<div class="schloopmark-like">
			<img src="<%= asset_path('admin/like_fill.svg') %>">
			<div>Shivam, Vaibhav & 5 others liked</div>
		</div>
		<div class="share-section">
			<div class="like-schloopmark share">
				<img src="<%= asset_path('admin/like.svg') %>">
				<p>Like</p>
			</div>
			<div class="write-comment share">
				<img src="<%= asset_path('admin/comment.svg') %>">
				<p>Comment</p>
			</div>
			<div class="add-Tomytopic share">
				<img src="<%= asset_path('admin/add_item.svg') %>">
				<p>Add to my topics</p>
			</div>
			<div class="follow-up-parent share">
				<img src="<%= asset_path('admin/follow_up.svg') %>">
				<p>Followup for parents</p>
			</div>
		</div>
		<div class="comment-section">
			{{#comments}}
			<div class="com-sub-section">
  				<div class="com-by-teacher-img" style="background-image: url('https://dummyimage.com/45x45/2abdda/0011ff&text=1');">
  				</div>
  				<div class="com-by-teacher-detail">
  					<div class="comment-as">
  					<span class="fw">Rucha Shah</span> <span>nice…we can also plan to take students to the planetarium…what say?</span>
  					</div>
  					<time class="timeago" datetime="date" title="id">20 mins ago </time>
  				</div>
			</div>
			{{/comments}}
			<div class="com-sub-section">
  				<div class="com-by-teacher-img" style="background-image: url('https://dummyimage.com/45x45/2abdda/0011ff&text=1');">
  				</div>
  				<div class="com-by-teacher-detail">
  					<div class="comment-editor col-md-12 p0">
				        <div class="write-comment" contenteditable="true" placeholder="Add a comment"></div>
				    </div>
  				</div>
			</div>			
		</div>
	</div>
	{{/data}}
</script>	