<table>
  <tr>
   	<th> title </th>
   	<th> body </th>
   	<th> type </th>
    <th> created by </th>
    <th> action </th>
  </tr>
<tbody>
<% @result.each do |circular| %>	
  <tr>
      <td>  <%= circular.title%></td>&nbsp
      <td> <%= circular.body %></td>
      <td>  <%= circular.circular_type%></td>
      <td>  <%= circular.created_by_type %></td>
      <td> <%= circular.created_at.to_date.to_s %>
      <td>
        <div class="thumbnail"> 
                    			<%= link_to 'show', '/admin/schools/(@school)/ecirculars/(@ecircular)' %>
        </div>
      </td>  
  </tr>
  <% end %>
</tbody>
</table>
   